-- character
function make_char(_x,_y)
	char={
		x=_x,
		y=_y,
		dx=0,
		maxdx=2,
		acc=0.5,		--walk accel
		frc=0.5,		--walk friction
		s={},     		--sprites
		sid=1,			--sprite index
		sdly=4,			--dly bet frames
		stime=0,
		w=2,
		h=2,
	}
end

function char_update()
	local s_stand={7,64}
	local s_left={66,68}
	local s_right={70,72}
	
	if btn(0) then
		if char.dx>-char.maxdx then
			char.dx-=char.acc
		end
		char.s=s_left
	elseif btn(1) then
		if char.dx<char.maxdx then
			char.dx+=char.acc
		end
		char.s=s_right
	else
		if char.dx!=0 then
			char.dx*=char.frc
		end
		char.s=s_stand
	end
	if not on_edge() then
		char.x+=char.dx
	else
		char.dx=0
	end
	char_anim()
end

function char_draw()
	spr(char.s[char.sid],char.x,char.y,char.w,char.h)
end

function char_anim()
	if char.stime<char.sdly then
		char.stime+=1
	else
		char.stime=0
		if char.sid<#char.s then
			char.sid+=1
		else
			char.sid=1
		end
	end
end

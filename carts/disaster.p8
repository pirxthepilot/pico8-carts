-- text instructions
function make_text()
	text={
		start_y=8,
		w={
			"\139 \145 to run",
			"\142 cry for help"
		},
		pressed=false,
	}
end

function text_update()
	if btn(0) or btn(1) or btn(4) then
		text.pressed=true
	end
end

function text_draw()
	if not text.pressed then
		for i=1,#text.w do
			local ind=64-#text.w[i]*2
			print(text.w[i],ind,text.start_y+(i*8)-8)
		end
	end
end

function on_edge()
	if
		(char.x<0 and not btn(1)) or
		(char.x+(char.w*8)>127 and not btn(0))
	then
		return true
	else
		return false
	end
end

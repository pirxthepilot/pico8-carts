-- help!
--	note: dependent on char
function make_help(_maxage)
	hlp={
		on=false,
		age=0,
		maxage=_maxage
	}
end

function help_update()
	if btnp(4) then
		hlp.on=true
		hlp.age=0
		sfx(1)
	else
		if hlp.age<hlp.maxage then
			hlp.age+=1
		else
			hlp.on=false
			hlp.age=0
		end
	end
end

function help_draw()
	if hlp.on then
		spr(9,char.x+16,char.y-16,2,2)
	end
end

